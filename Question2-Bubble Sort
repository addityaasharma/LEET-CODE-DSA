// learned bubble sort successfully
const arr=[48,47,49];
const sorted=(arr)=>{
    let length=arr.length;
    for(let i=0;i<length;i++){
        for(let j=0;j<length;j++){
            if(arr[i]<arr[j]){
                let temp=arr[i];
                arr[i]=arr[j];
                arr[j]=temp;
            };
        };
    };
    console.log(arr);
};

sorted(arr);
